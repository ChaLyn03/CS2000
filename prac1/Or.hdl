/**
 * Emulating OR operation using NAND and NOT gates.
 */

 CHIP OrUsingNandAndNot {
    IN a, b;
    OUT out;

    PARTS:
    // Negate both inputs 'a' and 'b'
    Not(in=a, out=nota);
    Not(in=b, out=notb);
    
    // Get the NAND of the negated inputs
    Nand(a=nota, b=notb, out=out);
}
